{
  "name": "Send a notification to PushOver",
  "version": "0.0.2",
  "type": "flogo:activity",
  "ref": "github.com/tuurleyck/flogo-pi/activity/pushover",
  "description": "Simple activity to send notifications to devices across PushOver Service",
  "author": "Arthur Eyckerman <arthur@aeyckerman.com>",
  "inputs":[
    {
      "name": "userKey",
      "type": "string"
    },
    {
      "name": "authToken",
      "type": "string"
    },
    {
      "name": "message",
      "type": "string"
    },
    {
      "name": "messageTitle",
      "type": "string"
    },
    {
      "name": "device",
      "type": "string"
    },
    {
      "name": "sound",
      "type": "string"
    },
    {
      "name": "url",
      "type": "string"
    },
    {
      "name": "urlTitle",
      "type": "string"
    },
    {
      "name": "priority",
      "type": "int",
      "value": 0
    }
  ],
  "outputs": [
  	{
      "name": "status",
      "type": "string"
    }
  ]
}